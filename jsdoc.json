{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./"],
    "exclude": ["lib", "node_modules", "out", "index.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "collapseSymbols": true,
    "copyright": "2017-2018 Ouest Syst√®mes Informatiques (OSInet)",
    "footer": "",
    "inverseNav": false,
    "linenums": true,
    "monospaceLinks": true,
    "navType": "vertical",
    "systemName": "meteor_server_info",
    "theme": "cerulean"
  },
  "opts": {
    "template": "node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "out",
    "recurse": true,
    "private": true
  }
}
